<?xml version="1.0" encoding="UTF-8"?>
<S-Program name="TestQuote">
    <S-Instructions>
        <!-- Main program P that calls function Q -->
        <S-Instruction type="synthetic" name="QUOTE">
            <S-Variable>z1</S-Variable>
            <S-Instruction-Arguments>
                <S-Instruction-Argument name="functionName" value="Q"/>
                <S-Instruction-Argument name="functionArguments" value="x1,y"/>
            </S-Instruction-Arguments>
        </S-Instruction>
        
        <!-- Some other instructions -->
        <S-Instruction type="basic" name="INCREASE">
            <S-Variable>z1</S-Variable>
        </S-Instruction>
        
        <S-Instruction type="basic" name="DECREASE">
            <S-Variable>x2</S-Variable>
        </S-Instruction>
        
        <S-Instruction type="basic" name="JUMP_NOT_ZERO">
            <S-Variable>x2</S-Variable>
            <S-Label>L1</S-Label>
            <S-Instruction-Arguments>
                <S-Instruction-Argument name="JNZLabel" value="L1"/>
            </S-Instruction-Arguments>
        </S-Instruction>
    </S-Instructions>
    
    <S-Functions>
        <S-Function name="Q" user-string="Q">
            <S-Instructions>
                <!-- Function Q that has its own variables and labels -->
                <S-Instruction type="basic" name="JUMP_ZERO">
                    <S-Variable>x1</S-Variable>
                    <S-Label>L1</S-Label>
                    <S-Instruction-Arguments>
                        <S-Instruction-Argument name="JZLabel" value="EXIT"/>
                    </S-Instruction-Arguments>
                </S-Instruction>
                
                <S-Instruction type="basic" name="DECREASE">
                    <S-Variable>x1</S-Variable>
                </S-Instruction>
                
                <S-Instruction type="basic" name="DECREASE">
                    <S-Variable>x2</S-Variable>
                </S-Instruction>
                
                <S-Instruction type="basic" name="INCREASE">
                    <S-Variable>z1</S-Variable>
                </S-Instruction>
                
                <S-Instruction type="basic" name="INCREASE">
                    <S-Variable>y</S-Variable>
                </S-Instruction>
                
                <S-Instruction type="basic" name="JUMP_NOT_ZERO">
                    <S-Variable>x1</S-Variable>
                    <S-Label>L1</S-Label>
                    <S-Instruction-Arguments>
                        <S-Instruction-Argument name="JNZLabel" value="L1"/>
                    </S-Instruction-Arguments>
                </S-Instruction>
            </S-Instructions>
        </S-Function>
    </S-Functions>
</S-Program>
