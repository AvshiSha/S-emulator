<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label style="-fx-font-size: 16px; -fx-font-weight: bold; -fx-padding: 10px;" text="History Chain" />
      <TableView fx:id="historyTableView" maxHeight="Infinity" maxWidth="Infinity" minHeight="200.0" minWidth="250.0" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="historyRowNumberColumn" minWidth="35.0" prefWidth="40.0" text="Row #" />
            <TableColumn fx:id="historyCommandTypeColumn" minWidth="25.0" prefWidth="30.0" text="B/S" />
            <TableColumn fx:id="historyLabelColumn" minWidth="35.0" prefWidth="40.0" text="Label" />
            <TableColumn fx:id="historyInstructionTypeColumn" minWidth="120.0" prefWidth="360.0" text="Command" />
            <TableColumn fx:id="historyCyclesColumn" minWidth="35.0" prefWidth="40.0" text="Cycles" />
         </columns>
      </TableView>
   </children>
</VBox>
