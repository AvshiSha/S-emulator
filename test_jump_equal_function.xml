<?xml version="1.0" encoding="UTF-8"?>
<S-Program name="TestJumpEqualFunction">
    <S-Instructions>
        <!-- Main program that uses JUMP_EQUAL_FUNCTION -->
        <S-Instruction type="synthetic" name="JUMP_EQUAL_FUNCTION">
            <S-Variable>x1</S-Variable>
            <S-Instruction-Arguments>
                <S-Instruction-Argument name="JEFunctionLabel" value="L1"/>
                <S-Instruction-Argument name="functionName" value="Const"/>
                <S-Instruction-Argument name="functionArguments" value=""/>
            </S-Instruction-Arguments>
        </S-Instruction>
        
        <!-- Some other instructions -->
        <S-Instruction type="basic" name="INCREASE">
            <S-Variable>x1</S-Variable>
        </S-Instruction>
        
        <S-Instruction type="basic" name="DECREASE">
            <S-Variable>x2</S-Variable>
        </S-Instruction>
        
        <S-Instruction type="basic" name="JUMP_NOT_ZERO">
            <S-Variable>x2</S-Variable>
            <S-Label>L1</S-Label>
            <S-Instruction-Arguments>
                <S-Instruction-Argument name="JNZLabel" value="L1"/>
            </S-Instruction-Arguments>
        </S-Instruction>
    </S-Instructions>
    
    <S-Functions>
        <S-Function name="Const" user-string="Const">
            <S-Instructions>
                <!-- Function that returns a constant value -->
                <S-Instruction type="synthetic" name="CONSTANT_ASSIGNMENT">
                    <S-Variable>y</S-Variable>
                    <S-Instruction-Arguments>
                        <S-Instruction-Argument name="constantValue" value="5"/>
                    </S-Instruction-Arguments>
                </S-Instruction>
            </S-Instructions>
        </S-Function>
    </S-Functions>
</S-Program>
